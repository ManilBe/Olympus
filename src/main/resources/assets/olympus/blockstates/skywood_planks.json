{
  "multipart": [
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "olympus:block/skywood_planks_post"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "olympus:block/skywood_planks",
        "uvlock": true
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "olympus:block/skywood_planks",
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "olympus:block/skywood_planks",
        "y": 270,
        "uvlock": true
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "olympus:block/skywood_planks",
        "y": 90,
        "uvlock": true
      }
    }
  ]
}